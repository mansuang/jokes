{
    "name": "mansuang/jokes",
    "description": "Create random jokes",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mansuang",
            "email": "mansaung@hotmail.com"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": "^6.3"
    },
    "autoload": {
        "psr-4": {
            "Mansuang\\Jokes\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mansuang\\Jokes\\Tests\\": "tests"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "orchestra/testbench": "~3.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mansuang\\Jokes\\ChuckNorrisJokesServiceProvider"
            ],
            "aliases": {
                "ChuckNorris": "Mansuang\\Jokes\\Facades\\ChuckNorris"
            }
        }
    }
}
